  <!-- Navbar -->
    <div class="container-fluid bclr text-light">
      <div class="row">
        <div class="col-2 mt-2 pl-5">
          <a  class="linktext" href="/"><span class="evntis">Eventista</span></a>
        </div>
        <div class="col-6 mt-2 text-left">
          <div class="inp">
            <div class="input-group mb-3">
              <div class="input-group-prepend">
                <span class="input-group-text" id="basic-addon1"
                  ><i class="fa fa-search" aria-hidden="true"></i
                ></span>
              </div>
              <input
                type="text"
