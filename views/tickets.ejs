    <img class="bg-image indexBGPastEvent" src="/Images/indexBG.png" alt="" />
    
 <div class="container page-content">
       <div class="main-body ">

               <h1 class="heading indent">My Tickets</h1>
        

          <!-- table starts over here -->
           <table class="table table-striped">

           <thead class="thead-dark">
          <tr >
            <th scope="col">#</th>
            <th scope="col">Event Name</th>
            <th scope="col">Date OF Event</th>
            <th scope="col">Type Of Ticket</th>
            <th scope="col">No. Of Tickets</th>
            <th scope="col">Total Price</th>
          </tr>
           </thead>

         <tbody>

            <% for(var i=0;i<ticketList.length;i++) { %> 
                <tr>
                    <th scope="row"><%= (i+1) %></th>
                    <td><%= ticketList[i].Ename %></td>
                    <td><%= DOEList[i] %> </td>
                    <td><%= ticketList[i].TypeOfticket %></td>
                    <td><%= ticketList[i].NOTickets %></td>
                    <td><%= ticketList[i].TotalPrice %></td>
                  </tr>

            <% } %> 


