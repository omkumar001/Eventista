    <section class="get-in-touch">
      <h1 class="title">Create Event</h1>
      <form class="contact-form row" action="/createEvent" method="POST">
         <div class="form-field col-lg-6">
            <input id="name" class="input-text js-input"name="eventname" type="text" placeholder="Event Name" required>
         </div>
         
         <div class="form-field col-lg-6 ">
            <input id="email" class="input-text js-input" name="category" type="text" placeholder="Category" required>
         </div>

         <div class="form-field col-lg-6">
          <input id="name" class="input-text js-input"name="venue" type="text" placeholder="Venue" required>
       </div>

       <div class="form-field col-lg-6">
        <input id="name" class="input-text js-input"name="maxParticipant" type="text" placeholder="Max. Attendees" required>
     </div>

     <div class="form-field col-lg-6">
      <input id="name" class="input-text js-input"name="goldTicketPrice" type="text" placeholder="Gold Ticket Price" required>
   </div>

   <div class="form-field col-lg-6">
    <input id="name" class="input-text js-input"name="diamondTicketPrice" type="text" placeholder="Diamond Ticket Price" required>
 </div>

 <div class="form-field col-lg-6">
  <input id="name" class="input-text js-input"name="platinumTicketPrice" type="text" placeholder="Platinum Ticket Price" required>
</div>

<div class="form-field col-lg-6">
  <input id="name" class="input-text js-input"name="posterURL" type="text" placeholder="Poster URL" required>
</div>

<div class="form-field col-lg-6">
  <input id="name" class="input-text js-input" type="date" name="DOE" placeholder="Date of Event" required/>
  <label class="label" for="name">Date of Event</label>
</div>

    <div class="form-field col-lg-6">
   <input id="name" class="input-text js-input" type="date" name="Deadline" placeholder="Event Deadline" required/>
    <label class="label" for="name">Event Deadline</label>
   </div>

    <div class="form-field col-lg-12">
    <textarea id="message" class="input-text js-input" name="eventDetails" id="" cols="60" rows="1" placeholder="Event Description Here" required></textarea> 
    </div>

    <div class="form-field col-lg-6 ">
      <input id="text" class="input-text js-input" name="insta" type="text" placeholder="Event Instagram Handle" required>
   </div>
  
   <div class="form-field col-lg-6 ">
    <input id="text" class="input-text js-input" name="fb" type="text" placeholder="Event Facebook Handle" required>
  </div>
  
  <div class="form-field col-lg-6 ">
    <input id="text" class="input-text js-input" name="twitter" type="text" placeholder="Event Twitter Handle" required>
   </div>

     <div class="form-field col-lg-12">
     <input class="submit-btn" type="submit" value="Submit">
    </div>

      </form>
   </section>

